<div id="<%= dom_id user %>">
  <p>
    <strong>Nome:</strong>
    <%= user.nome %>
  </p>

  <p>
    <strong>Matricula:</strong>
    <%= user.matricula %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Senha:</strong>
    <%= user.senha %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= user.token %>
  </p>

</div>
